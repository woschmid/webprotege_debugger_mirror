<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:text="urn:import:edu.stanford.bmir.protege.web.client.library.text"
>
    <ui:with field="msg" type="edu.stanford.bmir.protege.web.client.Messages"/>
    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>
    <ui:style>
        .langTagField {
        flex-grow: 1;
        flex-shrink: 1;
        }
        .resetButton {
        flex-grow: 0;
        flex-shrink: 0;
        margin-left: 7px;
        }
        .langTagPanel {
        display: flex;
        flex-direction: row;
        }
        .warningField {
        margin-top: 20px;
        }
    </ui:style>
    <g:HTMLPanel>
        <div class="{wp.style.formGroup}">
            <g:Label text="Debugger TimeOut" ui:field="TimeOutLabel" addStyleNames="{wp.style.formLabel}"/>
            <text:ExpandingTextBoxImpl ui:field="textBox"
                                       mode="MULTI_LINE"
                                       anchorVisible="false"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>