<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:text="urn:import:edu.stanford.bmir.protege.web.client.library.text"
>
    <ui:with field="msg" type="edu.stanford.bmir.protege.web.client.Messages"/>
    <ui:with field="wp" type="edu.stanford.bmir.protege.web.resources.WebProtegeClientBundle"/>
    <ui:style>
        .langTagField {
        flex-grow: 1;
        flex-shrink: 1;
        }
        .resetButton {
        flex-grow: 0;
        flex-shrink: 0;
        margin-left: 7px;
        }
        .langTagPanel {
        display: flex;
        flex-direction: row;
        }
        .warningField {
        margin-top: 20px;
        }
    </ui:style>
    <g:HTMLPanel>
        <div class="{wp.style.formGroup}">
            <g:Label text="Debugger Seesion Keep Time" ui:field="DSKTLabel" addStyleNames="{wp.style.formLabel}"/>
            <g:TextBox  ui:field="DSKTtextBox"/>
        </div>
        <div class="{wp.style.formGroup}">
            <g:Label text="Reasoner timeout" ui:field="RTLabel" addStyleNames="{wp.style.formLabel}"/>
            <g:TextBox  ui:field="RTtextBox"/>
        </div>
        <div class="{wp.style.formGroup}">
            <g:Label text="Max Visible Possibly Faulty Axioms" ui:field="MVPFALabel" addStyleNames="{wp.style.formLabel}"/>
            <g:TextBox  ui:field="MVPFAtextBox"/>
        </div>
        <div class="{wp.style.formGroup}">
            <g:Label text="Max visible correct axioms" ui:field="MVCALabel" addStyleNames="{wp.style.formLabel}"/>
            <g:TextBox  ui:field="MVCAtextBox"/>
        </div>
        <div class="{wp.style.formGroup}">
            <g:Label text="Reasoner" ui:field="ReasonerLabel" addStyleNames="{wp.style.formLabel}"/>
            <g:ListBox ui:field="ReseanerFild">
                <g:item value="HermiT">HermiT</g:item>
                <g:item value="pellet">pellet</g:item>
                <g:item value="jfact">jfact</g:item>
                <g:item value="JCEL">JCEL</g:item>
                <g:item value="Snorocket">Snorocket</g:item>
                <g:item value="ELK">ELK</g:item>
            </g:ListBox>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>